include ../../../components/blocks/mainDirection/mainDirectioninclude ../../../components/blocks/mainSeasonTicket/mainSeasonTicketinclude ../../../components/blocks/mainOurTeam/mainOurTeaminclude ../../../components/blocks/mainReview/mainReviewmixin main(options)  if !options    -options = {}  -    var classes = "main"        classes += options.element ? " " + options.element + "__main" : ""  main(class= classes)    +mainDirection({element: "main"})    +mainSeasonTicket({element: "main"})    +mainOurTeam({element: "main"})    +mainReview({element: "main"})