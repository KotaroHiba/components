include ../../items/pageSeparator/pageSeparatorinclude ../../items/menu/menuinclude ../../items/socialIcons/socialIconsmixin header()  if !options    -options = {}  -    var classes = "header"        classes += options.element ? " " + options.element + "__header" : ""  header(class= classes)    .header__top.header-container      +menu({element: "header"})      p.header__telephone-number 8 (812) 690-10-10    .header__center.header-container      p.header__heading-description Почувствуй расслабление в своем теле      h1.header__heading YOGA      p.header__offer Запишитесь на бесплатное <br> пробное занятие    .header__bottom.header-container      +socialIcons({element: "header"})      p.header__city-name Санкт-Петербург    +pageSeparator("01", {element: "header"})