include ../../items/logo/logoinclude ../../items/footer-list/footer-listinclude ../../items/input-text-field/input-text-fieldfooter.footer  +logo({element: "footer", widthText: true})  +footer-list({      element: "footer",      heading: "Навигация",      text: [          "О нас", "Новости", "Служба поддержки", "Услуги"      ]  })  +footer-list({      element: "footer",      heading: "О нас",      text: [          "О сервисе", "Наша команда", "Вакансии", "Инвесторы"      ]  })  +footer-list({      element: "footer",      heading: "Служба поддержки",      text: [          "Соглашения", "Сообщества", "Связь с нами"      ]  })  .footer__subscription-conteiner    +footer-list({        element: "footer",        addAnyClass: "footer__footer-list_column",        heading: "Подписка",        text: ["Получайте специальные предложения и новости сервиса"]    })    +input-text-field({        arrow: true,        element: "footer",        type: "email",        name: "footer-email-subscription",        id: "footer-subscription",        placeholder: "Email"    })