include ../../items/pageSeparator/pageSeparatorinclude ../../items/seasonTicketBlock/seasonTicketBlockmixin mainSeasonTicket(options)  if !options    -options = {}  -    var classes = "mainSeasonTicket"        classes += options.element ? " " + options.element + "__mainSeasonTicket" : ""  section(class= classes)    h2.mainSeasonTicket__heading Абонементы    .mainSeasonTicket__months-container      p.mainSeasonTicket__month        a(href="#").mainSeasonTicket__month-one 1 месяц        a(href="#").mainSeasonTicket__month-six 6 месяцев        a(href="#").mainSeasonTicket__month-twelve 12 месяцев    .mainSeasonTicket__blocks-container      +seasonTicketBlock({          element: "mainSeasonTicket",          mod: "morning",          time: "Утро",          explanation: "с 11:00 до 16:00",          price: "1 300 Р",          btn: "Заказать абонимент",      })      +seasonTicketBlock({          element: "mainSeasonTicket",          mod: "main",          main: true,          time: "Безлимит",          price: "2 800 Р",          btn: "Заказать абонимент",      })      +seasonTicketBlock({          element: "mainSeasonTicket",          mod: "evening",          time: "Вечер",          explanation: "с 17:00 до 21:00",          price: "2 000 Р",          btn: "Заказать абонимент",      })    +pageSeparator("03", {element: "mainSeasonTicket"})