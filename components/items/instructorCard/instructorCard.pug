include ../socialIcons/socialIconsmixin instructorCard(options)  if !options    -options = {}  -    var classes = "instructorCard"        classes += options.element ? " " + options.element + "__instructorCard": ""  section(class= classes)    img.instructorCard__img(src= options.imgSrc alt= options.imgAlt)    .instructorCard__description      p.instructorCard__instructor        span.instructorCard__instructor-word Инструктор        br        span.instructorCard__instructor-name= options.name      p.instructorCard__specialization= options.specialization      a.instructorCard__watch Смотреть      +socialIcons({element: "instructorCard"})