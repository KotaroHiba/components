//-           Options              |               Classes    ---------------------------------------------------------------------------    options.times - количество элементов списка    | options.element - является ли блок элементом    options.text - массив из текста для элементов  | options.mode - модификатор блока      Пример: text: [                              | options.state - модификатор состояния        "first-text",                              | options.addAnyClass - добавить любой класс        "second",        "more blood"      ]mixin bullet-list(options)  if !options    -options= {text: "You stupid girl(boy-girl(futanary))"}  -    var classes = 'bullet-list'    classes += options.element ? ' ' + options.element : ''    classes += options.mode ? ' bullet-list_' + options.mode : ''    classes += options.state ? ' bullet-list_state_' + options.state : ''    classes += options.addAnyClass ? ' ' + options.addAnyClass : ''  div(class=classes)   ul.bullet-list__conteiner    - for (var x = 0; x < options.times; x++)      each text in options.text        li.bullet-list__item= text