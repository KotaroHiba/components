mixin seasonTicketBlock(options)  if !options    -options = {}  -    var classes = "seasonTicketBlock"        classes += options.element ? " " + options.element + "__seasonTicketBlock": ""        classes += options.mod ? " " + options.element + "__seasonTicketBlock_" + options.mod : ""    var classesDescription = "seasonTicketBlock__description"        classesDescription += options.main ? " " + "seasonTicketBlock__description_main" : ""  section(class= classes)    div(class= classesDescription)      p.seasonTicketBlock__description-time        strong= options.time      if options.main        p.seasonTicketBlock__description-explanation Посещение не ограничено <br> по времени      else        p.seasonTicketBlock__description-explanation          span.seasonTicketBlock__description-explanation-visit Посещение          br          span.seasonTicketBlock__description-explanation-time= options.explanation      p.seasonTicketBlock__description-price        b= options.price    if options.main      btn.seasonTicketBlock__order-btn.seasonTicketBlock__order-btn_main= options.btn    else      btn.seasonTicketBlock__order-btn= options.btn